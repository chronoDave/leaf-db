{
  "name": "leaf-db",
  "version": "2.0.1",
  "description": "Small file-based database for node.js",
  "main": "src/model.js",
  "keywords": [
    "database",
    "embedded",
    "javascript",
    "node"
  ],
  "repository": "https://github.com/chronoDave/leaf-db.git",
  "author": "Chronocide",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "lint": "eslint . --ext .js --ignore-pattern node_modules",
    "test": "tape test/**/*.spec.js",
    "bench": "node test/benchmark.js",
    "bump:patch": "yarn version --patch && git push -u origin master --tags && yarn publish",
